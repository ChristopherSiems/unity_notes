{
    "manifest_version": 3,
    "name": "Unity Notes",
    "version": "1.0",
    "description": "An extension to add and request notes from selected text.",
    "permissions": ["contextMenus", "storage", "activeTab", "scripting"],
    "host_permissions": ["https://*/*", "http://*/*"],
    "background": {
        "service_worker": "background.js"
    }
    
}
